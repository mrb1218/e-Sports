<p id="notice"><%= notice %></p>

<h1>Seasons</h1>

<table>
  <thead>
    <tr>
      <th>Season name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @seasons.each do |season| %>
      <tr>
        <td><%= season.season_name %></td>
        <td><%= link_to 'Show', season %></td>
      </tr>
    <% end %>
  </tbody>
</table>
